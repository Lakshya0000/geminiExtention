{
    "manifest_version": 3,
    "name": "MCQ helper",
    "version": "1.0",
    "description": "AI tool which is will help you solving mcq questions with gemini",
     
    "permissions": [
        "contextMenus",
        "notifications",
        "storage"
      ],
      "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
      },
      "action": {
        "default_popup": "index.html",
        "default_icon": {
          "16": "icon16.png",
          "32": "icon32.png",
          "48": "icon48.png",
          "128": "icon128.png"
        }
      },
    "background": {
      "service_worker": "background.js"
    }
  
   
  }
  